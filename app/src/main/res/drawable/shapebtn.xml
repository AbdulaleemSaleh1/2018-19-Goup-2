<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="3dip" android:color="#293266" />
            <gradient android:angle="0" android:startColor="#58857e" android:endColor="#293266"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="3dip" android:color="#293266" />
            <solid android:color="#293266"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="8dip" />
            <stroke android:width="3dip" android:color="#293266" />
            <gradient android:angle="0" android:startColor="#abcdef" android:endColor="#abcdef" />
        </shape>
    </item>
</selector>